{
  "name": "simpixelated-starter",
  "version": "0.1.0",
  "description": "A boilerplate starter kit for React projects using ES6, Babel, ESLint, and Webpack.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./tooling/webpack.config.js --inline --quiet",
    "lint": "eslint ./src",
    "clean": "rm -rf ./dist/public",
    "prebuild": "npm run clean",
    "build": "webpack --config ./tooling/webpack.config.js --progress --colors --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simpixelated/react-es6-starter.git"
  },
  "keywords": [
    "react",
    "es6",
    "es2015",
    "babel",
    "webpack",
    "boilerplate",
    "starter"
  ],
  "author": "Jordan Kohl (https://twitter.com/simpixelated)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simpixelated/react-es6-starter/issues"
  },
  "homepage": "https://github.com/simpixelated/react-es6-starter#readme",
  "dependencies": {
    "lodash": "4.17.2",
    "moment": "^2.19.3",
    "react": "15.4.1",
    "react-dom": "15.4.1"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "css-loader": "0.26.1",
    "eslint": "3.12.2",
    "eslint-config-nativeaxis": "1.2.2",
    "eslint-plugin-lodash": "2.2.4",
    "eslint-plugin-react": "6.8.0",
    "html-webpack-plugin": "2.24.1",
    "style-loader": "0.13.1",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  }
}
